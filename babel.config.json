{
  "presets": [
    ["@babel/preset-env", { "targets": "defaults" }],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "@": "./src",
        },
      }
    ]
  ],
  "overrides": [
    {
      "test": "./src/pages/RegisterUser/RegisterUser.jsx", // Update this with the path to your specific file
      "sourceType": "module"
    }
  ]
}